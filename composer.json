{
    "name": "silktide/semrush-api",
    "description": "SEMrush API client",
    "require": {
        "php": ">=7.4|^8.0",
        "guzzlehttp/guzzle": "^6.1 | ^7.0",
        "psr/log": "^1|^2|^3",
        "psr/simple-cache": "^1.0",
        "silktide/capiture": "^1.2"
    },
    "require-dev": {
        "silktide/pharmacist": "^1.0",
        "phpunit/phpunit": "~8",
        "monolog/monolog": "^1.23",
        "cache/array-adapter": "^1.0"
    },
    "license": "GPL-2.0-or-later",
    "authors": [
        {
            "name": "Andrew Waite",
            "email": "andrewwaite@silktide.com"
        }
    ],
    "autoload": {
      "psr-4": {
        "Silktide\\SemRushApi\\": "src"
      }
    },
    "autoload-dev": {
        "psr-4": {
            "Silktide\\SemRushApi\\Test\\": "test"
        }
    },
    "extra": {
        "downsider-puzzle-di": {
            "silktide/syringe": {
                "path": "config/syringe.yml"
            }
        }
    }
}
